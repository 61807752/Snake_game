             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Snake_game"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundColor">#03021d</SolidColorBrush>
        <SolidColorBrush x:Key="GridBackgroundColor">#150633</SolidColorBrush>
        <SolidColorBrush x:Key="GridLineColor">#00ff00</SolidColorBrush>
        <SolidColorBrush x:Key="TextColor">#00ff00</SolidColorBrush>
        <SolidColorBrush x:Key="OverlayColor">#7F000000</SolidColorBrush>
        <FontFamily x:Key="MainFont">Assets/#Droid Sans Mono</FontFamily>
        
        
    </Application.Resources>
</Application>
